<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.simplemetalcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RoundPipeActivity" />
        <activity android:name=".ProfilePipeActivity" />
        <activity android:name=".RoundRodActivity" />
        <activity android:name=".ProfileRodActivity" />
        <activity android:name=".HexagonRodActivity" />
        <activity android:name=".SheetMetalActivity" />
        <activity android:name=".CornerActivity" />
        <activity android:name=".StripeActivity" />
        <activity android:name=".ChannelActivity" />
        <activity android:name=".GirdersActivity" />
        <activity android:name=".InfoActivity"></activity>
    </application>

</manifest>
